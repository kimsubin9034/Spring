			<ul>
				<li>10 + 2 =
					<span th:text="${10 + 2}"></span> <!-- 수정 -->
				</li>
				<li>10 % 2 =
					<span th:text="${10 % 2 == 0}"></span> <!-- 수정 -->
				</li>
			</ul>
		</li>
		<li> <!-- 수정 -->
			비교연산
			<ul>
				<li>1 > 10 =
					<span th:text="${1 > 10}"></span> <!-- 수정 -->
				</li>
				<li>
					1 gt 10 = <!-- 이 줄은 잘못된 비교 연산입니다. -->
					<span th:text="false"></span> <!-- 수정 -->
				</li>
			</ul>
		</li>
